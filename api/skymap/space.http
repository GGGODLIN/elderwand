# list spaces by project
GET {{HOST}}/api/spaces?projectId={{TARGET_PROJECT_CODE}}
accept: application/json
Content-Type: application/json

###


# get the space
GET {{HOST}}/api/spaces/{{TARGET_SPACE_ID}}?projectId={{TARGET_PROJECT_CODE}}
accept: application/json
Content-Type: application/json

###


# detele the space
DELETE {{HOST}}/api/spaces/{{TARGET_SPACE_ID}}?projectId={{TARGET_PROJECT_CODE}}
accept: application/json
Content-Type: application/json

###
