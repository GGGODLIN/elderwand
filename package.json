  "scripts": {
    "clear": "rm -rf .next/ .server/",
    "clear:lib": "rm -rf node_modules/ && yarn lib:unlink ",
    "clear:all": "yarn clear && yarn clear:lib && rm -f yarn.lock",
    "lib:link:install": "cd ../G13.Web.Shared && yarn install",
    "lib:link:create": "cd ../G13.Web.Shared/dist && yarn link",
    "lib:link": "yarn link g13-web-shared",
    "lib:unlink": "yarn unlink g13-web-shared",
    "shared:relink": "yarn lib:unlink && yarn lib:link:create && yarn lib:link",
    "web:analyze": "export ANALYZE=true && yarn web:build && yarn web:start",
    "web:copy": "sh scripts/handel-shared.sh",
    "web:install": "yarn install && sh scripts/handel-shared.sh",
    "web:dev": "export NODE_ENV=development && nodemon --config nodemon.json",
    "web:dev:sh": "export NODE_ENV=development && sh scripts/dev.sh",
    "web:dev:docker": "sh scripts/dev.sh",
    "web:build": "sh scripts/build.sh",
    "web:build:no-proxy": "sh scripts/build-no-proxy.sh",
    "web:start": "export NODE_ENV=production && node .server/src/server/index.js",
    "web:start:docker": "yarn web:build && export NODE_ENV=production && node .server/src/server/index.js",
    "json-server:init": "rm -f .temp/db.json && mkdir -p .temp/ && cp test/fixture/db.json .temp/db.json",
    "json-server": "sh scripts/json-server.sh",
    "major": "yarn version --pre --major",
    "minor": "yarn version --pre --minor",
    "patch": "yarn version --pre --patch"
